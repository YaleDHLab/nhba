    "eslint-plugin-react": "^7.6.1",
    "faker": "^4.1.0",
    "file-loader": "^0.10.1",
    "image-webpack-loader": "^3.3.0",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "prettier": "^1.10.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.14.1"

    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "express": "^4.14.0",
    "express-mongo-sanitize": "^1.3.2",
    "express-sanitizer": "^1.0.3",
    "express-session": "^1.14.1",
    "glob": "^7.1.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.6",
    "moment": "^2.18.1",
    "mongoose": "^4.9.2",
